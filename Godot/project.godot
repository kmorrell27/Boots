; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Zelda"
run/main_scene="res://core/client.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")
config/icon="res://editor/icon.png"

[autoload]

DEF="*res://core/definitions.gd"
ScreenFX="*res://core/vfx/screen_fx.tscn"
Sound="*res://core/sound.gd"
Inventory="*res://core/inventory.gd"
Dialogic="*res://addons/dialogic/Core/DialogicGameHandler.gd"

[debug]

gdscript/warnings/untyped_declaration=2
gdscript/warnings/inferred_declaration=1
gdscript/warnings/unsafe_property_access=2
gdscript/warnings/unsafe_method_access=2
gdscript/warnings/unsafe_cast=1
gdscript/warnings/unsafe_call_argument=2
gdscript/warnings/return_value_discarded=1

[dialogic]

directories/dch_directory={}
directories/dtl_directory={}

[display]

window/size/viewport_width=1024
window/size/viewport_height=576
window/stretch/aspect="expand"

[dotnet]

project/assembly_name="Zelda"

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogic/plugin.cfg")

